$("#submitnumber").click(function(){
var brand = $(".branded").val();
var model = $("#form_model").val();
var number = $("#number").val();
var name = $("#name").val();

if(brand == '' || brand == ' '){
	$("#branded").css("border-color","red");
	$("#error-brand").css("display","block");
	return false;
}
if(model == '' || model == ' '){
	$("#form_model").css("border-color","red");
	$("#error-model").css("display","block");
	return false;
}
if(name == '' || name == ' '|| $.isNumeric(name)){
	$("#name").css("border-color","red");
	$("#error-name").css("display","block");
	return false;
}
if(number == "" || !$.isNumeric(number) || (number == " ") || (number.charAt(0)!=9&&number.charAt(0)!=8&&number.charAt(0)!=7&&number.charAt(0)!=6)){
	$("#number").css("border-color","red");
	$("#error-number").css("display","block");
	return false;
}

$.ajax({
	url:"https://www.garage.movemycar.in/save-user-number",
	method:"POST",
	data:{brand:brand,model:model,name:name,number:number},
	   success:function(data)
	{
	// alert(data);
	// console.log(data);
		if(data == 'exist'){
			window.location.href = "https://www.garage.movemycar.in/home-form-search";
		}else{
			$("#submitnumber").hide();
			$("#submitotp").show();
			$(".user_otp").show();
		}
	}

});

});

